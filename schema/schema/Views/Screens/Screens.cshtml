@model IEnumerable<schema.View_Screen>
@{
    Layout = null;
}

<!DOCTYPE html>
<html style="height: 100%;">
<head>
    <title>智能导检大屏</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Scripts/js/Screen/css/index.css">
</head>
<body style="height: 100%;">
    <div class="bigscreen">
        <div class="bigscreen_top">
            <span>@ViewBag.departName</span>
        </div>
        <div class="bigscreen_main">
            <table border="1px" cellspacing="0" cellpadding="0">
                <th>诊室1</th>
                <th>诊室3</th>
                <th>诊室4</th>    
                <th>诊室7</th>           
                <tbody>
                    <tr class="current_check">
                        <td>当前检查：<span id="partname1">1</span></td>
                        <td>当前检查：<span id="partname2">@ViewBag.patien2</span></td>
                        <td>当前检查：<span id="partname3">@ViewBag.patien3</span></td> 
                        <td>当前检查：<span id="partname4">@ViewBag.patien3</span></td>                   
                    </tr>
                    <tr class="wait_check">
                        <td class="clearfix" colspan="4">
                            <span>候诊人：</span>
                            @foreach (var item in (List<View_Screen>)ViewBag.userkist)
                            {
                                <p>@item.NAME</p>
                            }
                           
                           
                        </td>

                    </tr>
                    <tr class="next_station">
                        <td>
                            <p>
                                <span>@ViewBag.patien1</span>下一站：<span>@ViewBag.depart1</span>
                            </p>

                        </td>
                        <td>
                            <p>
                                <span>@ViewBag.patien2</span>下一站：<span>@ViewBag.depart2</span>
                            </p>

                        </td>
                        <td>
                            <p>
                                <span>@ViewBag.patien3</span>下一站：<span>@ViewBag.depart3</span>
                            </p>

                        </td>
                        <td>
                            <p>
                                <span>@ViewBag.patien4</span>下一站：<span>@ViewBag.depart4</span>
                            </p>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        <audio id="audio"></audio>

    </div>
    alert(12)
    @Scripts.Render("~/Scripts/js/Screen/Calling.js")

</body>
</html>
<script type="text/javascript">
	console.log(typeof typeof(null));
</script>
